<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<bean id="helloWorld" class="com.albertsalud.spring.annotations.HelloWorld">
		<property name="message" value="Hola desde beans.xls" />
	</bean>
	
	<bean id="helloWorldPrinter" class="com.albertsalud.spring.annotations.HelloWorldPrinter">
	</bean>
	
	<!-- Permite trabajar con anotaciones -->
	<context:annotation-config />
</beans>
